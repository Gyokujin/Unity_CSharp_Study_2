using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class ComparisonOperator : MonoBehaviour
{
    int a = 4;
    int b = 8;

    void Start()
    {
        // 비교 연산자
        Debug.Log(a < b); // a값이 b보다 작으면 참
        Debug.Log(a <= b); // a값이 b보다 작거나 같으면 참
        Debug.Log(a > b); // a값이 b보다 크면 참
        Debug.Log(a >= b); // a값이 b보다 크거나 같으면 참
        Debug.Log(a == b); // a값이 b와 같으면 참
        Debug.Log(a != b); // a값이 b와 다르면 참
    }
}
