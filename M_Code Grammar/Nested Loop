using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class NestedLoop : MonoBehaviour
{
    void Start()
    {
        // 중첩 루프 : 반복문 안에 다시 반복문이 있는 것
        for (int i = 2; i < 10; i++)
        {
            for (int j = 1; j < 10; j++)
            {
                Debug.Log(i + " * " + j + " = " + (i * j));
            }
        }
    }
}
